---
import Layout from '@core/layouts/layout.astro';
import WithSidebar from '@core/layouts/with-sidebar.astro';
import TextParallax from 'modules/scroll-interactions/text-parallax/astro/text-parallax.astro';
---
<Layout>
  <main class="flex flex-col gap-16">
    <WithSidebar>
      <section
        class="flex-1 flex flex-col gap-16 w-full xl:w-3/4 h-screen justify-center items-center"
        slot="content"
      >
        <div class="max-w-full">
        </div> 
      </section>
    </WithSidebar>
  </main>
</Layout>