{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@core/*": [
        "modules/core/*"
      ],
      "@sidebar/*": [
        "modules/sidebar/*"
      ],
      "@micro-interactions/*": [
        "modules/micro-interactions/*"
      ],
      "@scroll-interactions/*": [
        "modules/scroll-interactions/*"
      ],
      "@content/*": [
        "content/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
  }
}